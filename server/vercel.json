{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "js/(.*)",
      "dest": "public/js/"
    },
    {
      "src": "css/(.*)",
      "dest": "public/css/"
    },
    {
      "src": "asset/(.*)",
      "dest": "public/asset/"
    },
    {
      "src": "Data/(.*)",
      "dest": "public/Data/"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
