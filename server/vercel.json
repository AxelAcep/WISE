{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/JS/(.*)",
      "dest": "/public/JS/"
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/"
    },
    {
      "src": "/asset/(.*)",
      "dest": "/public/asset/"
    },
    {
      "src": "/Data/(.*)",
      "dest": "/public/Data/"
    },
  ]
}
