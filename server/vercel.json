{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/js/(.*)",
      "dest": "public/js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "public/css/$1"
    },
    {
      "src": "/asset/(.*)",
      "dest": "public/asset/$1"
    },
    {
      "src": "/Data/(.*)",
      "dest": "public/Data/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
