{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/JS/(.*)",
      "destination": "/public/JS/"
    },
    {
      "source": "/css/(.*)",
      "destination": "/public/css/"
    },
    {
      "source": "/asset/(.*)",
      "destination": "/public/asset/"
    },
    {
      "source": "/Data/(.*)",
      "destination": "/public/Data/"
    }
  ]
}
